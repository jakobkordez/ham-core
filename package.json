		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@types/d3-geo": "^3.1.0",
		"@types/eslint": "^8.56.7",
		"@types/topojson-client": "^3.1.4",
		"autoprefixer": "^10.4.19",
		"d3-geo": "^3.1.1",
		"daisyui": "^4.12.10",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"fast-dxcc": "^2.1.0",
		"globals": "^15.0.0",
		"postcss": "^8.4.39",

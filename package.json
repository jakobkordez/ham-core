		"prettier-plugin-tailwindcss": "^0.6.5",
		"supabase": "^1.183.5",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-fa": "^4.0.2",
		"tailwindcss": "^3.4.4",
		"topojson-client": "^3.1.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"type": "module"
